@import term
@import mem

biosServiceAdr u32

copyEax raw<"=r"> -> u32
    mov eax, eax
;

@export kstart fun
    biosServiceAdr <- copyEax()
    @term.init(biosServiceAdr, 0)
    @term.printString("Initializing Bits Runner...\n", FOREGROUND_GREEN.u8)
    @mem.init()
    @term.printString("All ready, welcome to Bits Runner!\n", 7)

    rep
    ;
;